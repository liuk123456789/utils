{
  "name": "@konalau/utils",
  "type": "module",
  "version": "1.0.6",
  "packageManager": "pnpm@9.15.3",
  "description": "Apply for Project",
  "author": "Kona <liuer5194@gmail.com>",
  "homepage": "https://github.com/liuk123456789/utils#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/liuk123456789/utils.git"
  },
  "keywords": [
    "utils"
  ],
  "sideEffects": false,
  "exports": {
    ".": {
      "import": "./dist/index.mjs",
      "require": "./dist/index.cjs"
    }
  },
  "main": "dist/index.cjs",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "rollup -c",
    "dev": "nr build --watch",
    "lint": "eslint .",
    "lint-fix": "nr lint --fix",
    "prepublishOnly": "npm run build",
    "release": "bumpp --commit --push --tag && npm publish --access public",
    "start": "esno src/index.ts",
    "typecheck": "tsc --noEmit",
    "test": "vitest"
  },
  "dependencies": {
    "@ctrl/tinycolor": "^4.1.0",
    "dayjs": "^1.11.13"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^3.12.2",
    "@antfu/ni": "^0.23.2",
    "@rollup/plugin-alias": "^5.1.1",
    "@rollup/plugin-commonjs": "^28.0.2",
    "@rollup/plugin-json": "^6.1.0",
    "@rollup/plugin-node-resolve": "^16.0.0",
    "@types/node": "^22.10.5",
    "@types/throttle-debounce": "^5.0.2",
    "bumpp": "^9.9.3",
    "eslint": "^8.57.1",
    "esno": "^4.8.0",
    "rollup": "^4.30.1",
    "rollup-plugin-dts": "^6.1.1",
    "rollup-plugin-esbuild": "^6.1.1",
    "throttle-debounce": "5.0.0",
    "typescript": "^5.7.3",
    "vite": "^6.0.7"
  }
}
